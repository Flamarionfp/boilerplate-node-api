{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "allowJs": true,
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}